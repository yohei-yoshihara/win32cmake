  <Application.Views>
    <Ribbon>
      <!-- Application Menu -->
      <Ribbon.ApplicationMenu>
        <ApplicationMenu CommandName="FileMenu">
          <ApplicationMenu.RecentItems>
            <RecentItems CommandName="MRUList" MaxCount="16" EnablePinning="false"/>
          </ApplicationMenu.RecentItems>
          <MenuGroup Class="MajorItems">
            <Button CommandName="New"/>
            <!-- Open File SplitButton -->
            <SplitButton CommandName="OpenFileMore">
              <SplitButton.ButtonItem>
                <Button CommandName="OpenFile"/>
              </SplitButton.ButtonItem>
              <SplitButton.MenuGroups>
                <MenuGroup Class="MajorItems">
                  <Button CommandName="OpenFile"/>
                  <Button CommandName="OpenFreeMind"/>
                </MenuGroup>
              </SplitButton.MenuGroups>
            </SplitButton>
            <Button CommandName="Save"/>
            <Button CommandName="SaveAs"/>
          </MenuGroup>
          <MenuGroup Class="MajorItems">
            <!-- Printing SplitButton -->
            <SplitButton CommandName="PrintMore">
              <SplitButton.ButtonItem>
                <Button CommandName="Print" />
              </SplitButton.ButtonItem>
              <SplitButton.MenuGroups>
                <MenuGroup Class="MajorItems">
                  <Button CommandName="PrintSetup" />
                  <Button CommandName="Print" />
                </MenuGroup>
              </SplitButton.MenuGroups>
            </SplitButton>
          </MenuGroup>
          <MenuGroup Class="MajorItems">
            <Button CommandName="cmdVersionInfo"/>
          </MenuGroup>
          <MenuGroup Class="MajorItems">
            <Button CommandName="Exit"/>
          </MenuGroup>
        </ApplicationMenu>
      </Ribbon.ApplicationMenu>
      <Ribbon.QuickAccessToolbar>
        <QuickAccessToolbar CommandName="QAT">
        </QuickAccessToolbar>
      </Ribbon.QuickAccessToolbar>
      <Ribbon.Tabs>
        <Tab CommandName="cmdTabHome">
          <Group CommandName="GroupClipboard" SizeDefinition="BigButtonsAndSmallButtonsOrInputs">
            <ControlGroup>
              <Button CommandName="Paste"/>
            </ControlGroup>
            <ControlGroup>
              <Button CommandName="Cut"/>
              <Button CommandName="Copy"/>
              <Button CommandName="Undo"/>
            </ControlGroup>
          </Group>
          <Group CommandName="cmdFontControlGroup" SizeDefinition="OneFontControl">
            <FontControl CommandName="cmdFontControl" FontType="FontWithColor" ShowVerticalFonts="false"/>
          </Group>
          <Group CommandName="cmdBackgroundColorGroup" SizeDefinition="TwoButtons">
            <DropDownGallery CommandName="cmdNodeShape"
                             TextPosition="Hide"
                             Type="Items"
                             ItemHeight="64"
                             ItemWidth="64">
              <DropDownGallery.MenuLayout>
                <VerticalMenuLayout Gripper="None" />
              </DropDownGallery.MenuLayout>
            </DropDownGallery>
            <DropDownColorPicker ColorTemplate="ThemeColors" CommandName="cmdBackgroundColor" ChipSize="Large"/>
          </Group>
          <Group CommandName="cmdLineAttributeGroup" SizeDefinition="FourButtons">
            <DropDownColorPicker CommandName="cmdLineColor" 
                                 ColorTemplate="ThemeColors" 
                                 ChipSize="Large"/>
            <DropDownGallery CommandName="cmdLineWidth"
                             TextPosition="Hide"
                             Type="Items"
                             ItemHeight="24"
                             ItemWidth="64">
              <DropDownGallery.MenuLayout>
                <VerticalMenuLayout Gripper="None" />
              </DropDownGallery.MenuLayout>
            </DropDownGallery>
            <DropDownGallery CommandName="cmdLineEdgeStyle"
                             TextPosition="Hide"
                             Type="Items"
                             ItemHeight="24"
                             ItemWidth="64">
              <DropDownGallery.MenuLayout>
                <VerticalMenuLayout Gripper="None" />
              </DropDownGallery.MenuLayout>
            </DropDownGallery>
            <DropDownGallery CommandName="cmdLineStyle"
                             TextPosition="Hide"
                             Type="Items"
                             ItemHeight="24"
                             ItemWidth="64">
              <DropDownGallery.MenuLayout>
                <VerticalMenuLayout Gripper="None" />
              </DropDownGallery.MenuLayout>
            </DropDownGallery>
          </Group>
          <Group CommandName="GroupBasicOperationGroup" SizeDefinition="ThreeButtons">
            <Button CommandName="EditNode"/>
            <Button CommandName="CreateChildNode"/>
            <ToggleButton CommandName="cmdCreateLink"/>
          </Group>
        </Tab>
      </Ribbon.Tabs>
    </Ribbon>
    <ContextPopup>
      <ContextPopup.MiniToolbars>
        <MiniToolbar Name="myMiniToolbar">
          <MenuGroup Class="StandardItems">
            <FontControl CommandName="cmdFontControl" ShowVerticalFonts="false"/>
          </MenuGroup>
        </MiniToolbar>
      </ContextPopup.MiniToolbars>
      <ContextPopup.ContextMenus>
        <ContextMenu Name="ContextMenu1">
          <MenuGroup>
            <Button CommandName="EditNode"/>
            <Button CommandName="CreateChildNode"/>
            <Button CommandName="DeleteNode"/>
            <Button CommandName="cmdExpandNodes"/>
            <Button CommandName="cmdCollapseNodes"/>
          </MenuGroup>
          <MenuGroup>
            <ToggleButton CommandName="cmdCreateLink"/>
            <Button CommandName="cmdDeleteLink"/>
          </MenuGroup>
          <MenuGroup>
            <Button CommandName="Cut"/>
            <Button CommandName="Copy"/>
            <Button CommandName="Paste"/>
            <Button CommandName="Undo"/>
          </MenuGroup>
        </ContextMenu>
      </ContextPopup.ContextMenus>
      <ContextPopup.ContextMaps>
        <ContextMap CommandName="cmdContextMap1" 
                    MiniToolbar="myMiniToolbar" 
                    ContextMenu="ContextMenu1"/>
      </ContextPopup.ContextMaps>
    </ContextPopup>
  </Application.Views>
</Application>
